<h1>Listing foods</h1>

<div id="new">
				<div class="link"><%= link_to 'New Food', new_food_path %>
				</div>
				<div class="link"><%= link_to 'Get Data', foods_parser_path  %>
				</div>
</div>

<table>
  <tr>
    <th>Name</th>
    <th>Englishname</th>
    <th>Water</th>
    <th>Fat</th>
    <th>Proteins</th>
    <th>Energy</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @foods.each do |food| %>
  <tr>
    <td><%= food.name %></td>
    <td><%= food.englishName %></td>
    <td><%= food.water %></td>
    <td><%= food.fat %></td>
    <td><%= food.proteins %></td>
    <td><%= food.energy %></td>
    <td><%= link_to 'Show', food %></td>
    <td><%= link_to 'Edit', edit_food_path(food) %></td>
    <td><%= link_to 'Destroy', food, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

